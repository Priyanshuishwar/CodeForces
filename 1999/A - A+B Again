import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t -- > 0){
		    int n = sc.nextInt();
		    int a = n % 10;
		    n = n / 10;
		    int b = n % 10;
		    int ans = 0;
		    ans = a + b;
		    System.out.println(ans);
		}
	}
}