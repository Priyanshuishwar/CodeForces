import java.util.*;public class Main{	public static void main(String[] args) {		Scanner sc = new Scanner(System.in);		int t = sc.nextInt();		while(t -- > 0){		    int n = sc.nextInt();		    int arr[] = new int[n];		    long sum = 0;		    for(int i = 0; i < n; i++){		        arr[i] = sc.nextInt();		        sum += arr[i];		    }		    long sqr = (long)Math.sqrt(sum);		    if(sqr * sqr == sum){		        System.out.println("Yes");		    }		    else{		        System.out.println("No");		    }		}	}}